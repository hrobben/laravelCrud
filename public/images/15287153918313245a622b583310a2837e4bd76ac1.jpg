    });

    $('#load-area').css({'background-color': '#181817', 'border': 0});
    $('#load-area span').css({'color': 'white'});

    $('#show-code').click(function(){
        $('#show-code-content').css({
            top: 250 + 'px',
            left: $(document).width()/2 - 280 + 'px'
